@{
    ViewBag.Title = "Home Page";
}
@model List<MVCClient.Models.EmployeeModel>



<div class="row">

<h2>Employee Details</h2>
<div>
    <table border="1" style="width:100%">
        <tr>
            <th>ID</th>
            <th>EmployeeName</th>
            <th>Designation</th>
        </tr>
        @foreach (var employee in Model) {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Designation</td>
            </tr>
        }

    </table>
    </div>